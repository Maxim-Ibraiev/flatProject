{
  "printWidth": 110,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "[typescript]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false
  }
}
